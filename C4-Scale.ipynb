    "import json\n",
    "from pyecharts.charts import Tree\n",
    "import pyecharts.options as opts\n",
    "\n",
    "with open('./data/GDP_data.json', 'r', encoding='utf-8') as f:\n",
    "    j = json.load(f)\n",
    "    data = [j]\n",
    "\n",
    "tree = Tree(\n",
    "    init_opts=opts.InitOpts(width='800px', height='600px')# 页面允许的话画布可以给大一点 数据不要挤在一起\n",
    ")\n",
    "tree.add('', data)\n",
    "\n",
    "tree.render_notebook()\n",
    "\n",
    "#json本身结构化做的很好！可以看看json"
   ]
  },
